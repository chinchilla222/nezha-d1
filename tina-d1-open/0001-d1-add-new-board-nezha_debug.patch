+ */
+
+&platform {
+	eraseflag   = <1>;
+	next_work   = <3>;
+	debug_mode  = <1>;
+};
+
+&target {
+	boot_clock = <1008>;	/*CPU boot frequency, Unit: MHz*/
+	storage_type = <0xffffffff>; /*boot medium, 0-nand, 1-card0, 2-card2, -1(defualt)auto scan*/
+	burn_key = <0>;		/*1:support burn key; 0:not support burn key*/
+	dragonboard_test = <0>;	/*1:support card boot dragonboard; 0:not support card boot dragonboard*/
+};
+
+
+&charger0 {
+	pmu_safe_vol = <3500>;
+};
+
+
+&twi6 {
+	clock-frequency = <200000>;
+	pinctrl-0 = <&s_twi0_pins_a>;
+	no_suspend = <1>;
+	twi_drv_used = <1>;
+	status = "okay";
+};
+
+&pwm2_pin_a {
+	pins = "PD18";
+	function = "pwm2";
+	muxsel = <5>;
+	drive-strength = <10>;
+	bias-pull-up;
+};
+
+&pwm2_pin_b {
+	pins = "PD18";
+	muxsel = <0>;
+	function = "gpio_out";
+};
+
+&pwm2 {
+	pinctrl-names = "active", "sleep";
+	pinctrl-0 = <&pwm2_pin_a>;
+	pinctrl-1 = <&pwm2_pin_b>;
+	status = "okay";
+};
+
+&pwm7_pin_a {
+	pins = "PD22";
+	function = "pwm7";
+	muxsel = <5>;
+	drive-strength = <10>;
+	bias-pull-up;
+};
+
+&pwm7_pin_b {
+	pins = "PD22";
+	muxsel = <0>;
+	function = "gpio_in";
+};
+
+&pwm7 {
+	pinctrl-names = "active", "sleep";
+	pinctrl-0 = <&pwm7_pin_a>;
+	pinctrl-1 = <&pwm7_pin_b>;
+	status = "okay";
