+jtag_enable     = 0
+jtag_ms         = port:PF0<4><default><default><default>
+jtag_ck         = port:PF5<4><default><default><default>
+jtag_do         = port:PF3<4><default><default><default>
+jtag_di         = port:PF1<4><default><default><default>
