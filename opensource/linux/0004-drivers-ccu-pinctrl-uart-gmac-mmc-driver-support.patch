+			  unsigned long nr_rdump,
+			  const struct ccu_reg_dump *rsuspend,
+			  unsigned long nr_rsuspend)
+{ }
+#endif
+EXPORT_SYMBOL_GPL(sunxi_ccu_sleep_init);
 
